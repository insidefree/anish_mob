@import "variables"

@font-face
  font-family 'anish'
  src url(icomoon-font-path + '/anish.ttf?gm00gu') format('truetype'), url(icomoon-font-path + '/anish.woff?gm00gu') format('woff'), url(icomoon-font-path + '/anish.svg?gm00gu#anish') format('svg')
  font-weight normal
  font-style normal

[class^="yicon-"], [class*=" yicon-"]
  font-family 'anish' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.yicon-shelters
  &:before
    content yicon-shelters
.yicon-fosters
  &:before
    content yicon-fosters

